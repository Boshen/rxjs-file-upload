{
  "name": "rxjs-file-upload",
  "version": "2.0.1",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": "Boshen Chen <boshenc@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Boshen/rxjs-file-upload.git"
  },
  "files": ["dist/*", "src/*"],
  "scripts": {
    "prepare": "husky install",
    "start": "vite ./test",
    "lint": "pnpm eslint src/*.ts",
    "page": "rm -rf page && vite build ./test",
    "deploy": "pnpm run page && gh-pages -d page",
    "watch": "karma start",
    "test": "karma start --single-run",
    "build": "rm -rf dist && tsc",
    "prettier": "prettier --write './{src,test}/**/*.ts'"
  },
  "peerDependencies": {
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.4",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "chai": "^4.3.4",
    "css-loader": "^6.4.0",
    "eslint": "8.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.2",
    "lint-staged": "^11.2.3",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "rxjs": "^7.4.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tslib": "^1.10.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.7"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "always",
    "parser": "typescript"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": ["eslint --cache --fix"],
    "*.json": ["prettier --parser=json --write"]
  }
}
