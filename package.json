{
  "name": "rxjs-file-upload",
  "version": "2.1.1",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": "Boshen Chen <boshenc@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Boshen/rxjs-file-upload.git"
  },
  "files": ["dist/*", "src/*"],
  "scripts": {
    "prepare": "husky install",
    "start": "vite ./test",
    "lint": "eslint src/*.ts",
    "watch": "karma start",
    "test": "karma start --single-run",
    "build": "rm -rf dist && tsc",
    "prettier": "prettier --write './{src,test}/**/*.ts'"
  },
  "peerDependencies": {
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "chai": "^4.3.4",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^7.0.4",
    "karma": "^6.3.8",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.2",
    "lint-staged": "^11.2.6",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1",
    "rxjs": "^7.4.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.13"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "always",
    "parser": "typescript"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": ["eslint --cache --fix"],
    "*.json": ["prettier --parser=json --write"]
  }
}
