{
  "name": "rxjs-file-upload",
  "version": "1.3.1",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": "Boshen Chen <boshenc@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Boshen/rxjs-file-upload.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.js",
    "lint": "tslint src/*.ts",
    "page": "rm -rf page && webpack --config webpack/page.js",
    "deploy": "yarn run page && gh-pages -d page",
    "watch": "karma start",
    "test": "karma start --single-run",
    "build": "rm -rf dist && yarn lint && tsc",
    "prettier": "prettier --write './{src,test}/**/*.ts'"
  },
  "peerDependencies": {
    "rxjs": "^6.5.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.4",
    "chai": "^4.2.0",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "css-loader": "^3.5.3",
    "gh-pages": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "karma": "^5.0.9",
    "karma-es6-shim": "^1.0.0",
    "karma-mocha": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^5.0.3",
    "karma-verbose-reporter": "^0.0.6",
    "mocha": "^7.2.0",
    "prettier": "^2.0.5",
    "rxjs": "^6.5.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tslib": "^1.10.0",
    "tslint": "^6.1.2",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.2.4",
    "webpack": "^4.43.0",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "arrowParens": "always",
    "parser": "typescript"
  }
}
